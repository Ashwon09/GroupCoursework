@{
    ViewData["Title"] = "MmemberListNotBorrowed";
}
<style>
body{
background-image:url("../images/12.jpg");
background-repeat:no-repeat;
background-size:cover;
}
th{
    color:white;
}
</style>
<body>
<h1 class="text-center" style="font-weight:800; color: #F1C381;">Members list who have not borrowed any DVD in last 32 days</h1>
<br />
    <div class="container">
        <div class="card" style="background-color:#A96536; opacity:70%;">
            <table class="table" style="color:white;">
                <thead>
                    <tr>
                        <th>MembershipFirstName</th>
                        <th>MembershipLastName</th>
                        <th>MembershipAddress</th>
                        <th>Date out of most recent loan</th>
                        <th>Difference</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in ViewBag.details)
                    {
                var t = DateTime.Now.ToShortDateString();
                var d = @item.MaxDates;
                var diff = (DateTime.Parse(t) - DateTime.Parse(d)).Days;

                <tr>
                            <td>@item.MembershipFirstName</td>
                            <td>@item.MembershipLastName</td>
                            <td>@item.MembershipAddress</td>
                            <td>@item.MaxDates</td>
                            <td>@diff
                            </td> 
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</body>